{
    "pages": [
        {
            "path": "/account/:address",
            "name": "account-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "accountDetailTitle",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "address",
                            "accountAliasName",
                            "addressHeight",
                            "publicKey",
                            "importance",
                            "type",
                            "linkedAccountKey"
                        ],
                        "managerGetter": "account/info",
                        "pagination": false,
                        "errorMessage": "accountDetailError"
                    },
                    {
                        "type": "Card",
                        "title": "multisigCosignatoriesTitle",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "fields": [
                            "minApproval",
                            "minRemoval",
                            "cosignatories",
                            "multisigAccounts"
                        ],
                        "managerGetter": "account/multisig",
                        "pagination": false,
                        "errorMessage": "multisigCosignatoriesError"
                    },
                    {
                        "type": "Card",
                        "title": "ownedMosaicsTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "managerGetter": "account/OwnedMosaic",
                        "fields": [
                            "mosaicId",
                            "amount",
                            "mosaicAliasName"
                        ],
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "ownedMosaicsError"
                    },
                    {
                        "type": "Card",
                        "title": "ownedNamespacesTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "managerGetter": "account/OwnedNamespace",
                        "fields": [
                            "namespaceName",
                            "status",
                            "duration",
                            "registrationType"
                        ],
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "ownedNamespacesError"
                    },
                    {
                        "type": "Card",
                        "title": "supplementalKeysTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "accountKeyType",
                            "key"
                        ],
                        "managerGetter": "account/info",
                        "dataGetter": "account/getSupplementalAccountKeys",
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "supplementalKeysError"
                    },
                    {
                        "type": "Card",
                        "title": "accountTransactionsTitle",
                        "fullWidth": true,
                        "header": [
                            {
                                "type": "filter",
                                "filterGetter": "account/transactions"
                            }
                        ],
                        "body": "TableListView",
                        "fields": [
                            "deadline",
                            "transactionHash",
                            "transactionDescriptor"
                        ],
                        "managerGetter": "account/transactions",
                        "timelinePagination": true,
                        "errorMessage": "transactionsError"
                    },
                    {
                        "type": "Card",
                        "title": "accountPartialTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "managerGetter": "account/partialTransactions",
                        "fields": [
                            "signer",
                            "transactionType",
                            "transactionHash",
                            "deadline",
                            "transactionBody"
                        ],
                        "timelinePagination": true,
                        "errorMessage": "accountPartialError"
                    },
                    {
                        "type": "Card",
                        "title": "accountRestrictionTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "managerGetter": "account/restrictions",
                        "pagination": false,
                        "pageSize": 5,
                        "errorMessage": "Account restrictions are not available."
                    },
                    {
                        "type": "Card",
                        "title": "metadataEntriesTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadataId",
                            "scopedMetadataKey",
                            "senderAddress",
                            "targetAddress",
                            "metadataValue"
                        ],
                        "managerGetter": "account/metadatas",
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "metadataEntriesError"
                    },
                    {
                        "type": "Card",
                        "title": "importanceHistoryTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "recalculationBlock",
                            "totalFeesPaid",
                            "beneficiaryCount",
                            "importanceScore"
                        ],
                        "managerGetter": "account/info",
                        "dataGetter": "account/getActivityBucketList",
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "importanceHistoryError"
                    }
                ]
            }
        },
        {
            "path": "/block/:height",
            "name": "block-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "blockDetailTitle",
                        "fullWidth": true,
                        "header": [
                            {
                                "type": "Pagination",
                                "canFetchPrevious": true,
                                "canFetchNext": true,
                                "nextPageAction": "block/nextBlock",
                                "previousPageAction": "block/previousBlock"
                            }
                        ],
                        "body": "TableInfoView",
                        "fields": [
                            "height",
                            "date",
                            "totalFee",
                            "difficulty",
                            "feeMultiplier",
                            "transactions",
                            "harvester",
                            "blockHash",
                            "proofGamma",
                            "proofScalar",
                            "proofVarificationHash"
                        ],
                        "pagination": false,
                        "managerGetter": "block/info",
                        "errorMessage": "blockDetailError"
                    },
                    {
                        "type": "Card",
                        "title": "blockTransactionsTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "header": [
                            {
                                "type": "filter",
                                "filterGetter": "block/transactions"
                            }
                        ],
                        "fields": [
                            "deadline",
                            "transactionId",
                            "transactionHash",
                            "transactionDescriptor"
                        ],
                        "managerGetter": "block/transactions",
                        "timelinePagination": true,
                        "errorMessage": "blockTransactionsError"
                    },
                    {
                        "type": "Card",
                        "title": "balanceChangeReceiptTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/balanceChangeReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": false,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "balanceChangeReceiptError"
                    },
                    {
                        "type": "Card",
                        "title": "balanceTransferReceiptTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/balanceTransferReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "balanceTransferReceiptError"
                    },
                    {
                        "type": "Card",
                        "title": "inflationReceiptTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/inflationReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "inflationReceiptError"
                    },
                    {
                        "type": "Card",
                        "title": "artifactExpiryReceiptTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/artifactExpiryReceipt",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "artifactExpiryReceiptError"
                    },
                    {
                        "type": "Card",
                        "title": "resolutionStatementTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "dataGetter": "block/resolutionStatement",
                        "managerGetter": "block/blockReceipts",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "resolutionStatementError"
                    }
                ]
            }
        },
        {
            "path": "/mosaic/:mosaicId",
            "name": "mosaic-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "mosaicDetailTitle",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "mosaicId",
                            "mosaicAliasName",
                            "divisibility",
                            "address",
                            "supply",
                            "relativeAmount",
                            "revision",
                            "startHeight",
                            "duration",
                            "supplyMutable",
                            "transferable",
                            "restrictable"
                        ],
                        "managerGetter": "mosaic/info",
                        "pagination": false,
                        "errorMessage": "mosaicDetailError"
                    },
                    {
                        "type": "Card",
                        "title": "mosaicRestrictionTitle",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "fields": [
                            "compositeHash",
                            "entryType"
                        ],
                        "managerGetter": "mosaic/restrictions",
                        "pagination": false,
                        "errorMessage": "mosaicRestrictionError"
                    },
                    {
                        "type": "Card",
                        "title": "mosaicRestrictionListTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "restrictionKey",
                            "restrictionType",
                            "restrictionValue"
                        ],
                        "managerGetter": "mosaic/restrictions",
                        "dataGetter": "mosaic/getMosaicRestrictionList",
                        "pagination": false,
                        "errorMessage": "mosaicRestrictionListError"
                    },
                    {
                        "type": "Card",
                        "title": "metadataEntriesTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadataId",
                            "scopedMetadataKey",
                            "targetId",
                            "senderAddress",
                            "targetAddress",
                            "metadataValue"
                        ],
                        "managerGetter": "mosaic/metadatas",
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "metadataEntriesError"
                    }
                ]
            }
        },
        {
            "path": "/namespace/:namespaceId",
            "name": "namespace-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "namespaceDetailTitle",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "owneraddress",
                            "namespaceName",
                            "namespaceId",
                            "registrationType",
                            "status",
                            "aliasType",
                            "aliasAddress",
                            "aliasMosaic",
                            "startHeight",
                            "expiredInBlock",
                            "beforeEndHeight",
                            "endHeight"
                        ],
                        "managerGetter": "namespace/info",
                        "pagination": false,
                        "errorMessage": "namespaceDetailError"
                    },
                    {
                        "type": "Card",
                        "title": "namespaceLevelTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "name",
                            "namespaceId",
                            "parentId"
                        ],
                        "managerGetter": "namespace/namespaceLevel",
                        "pagination": false,
                        "errorMessage": "namespaceLevelError"
                    },
                    {
                        "type": "Card",
                        "title": "metadataEntriesTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "metadataId",
                            "scopedMetadataKey",
                            "targetId",
                            "senderAddress",
                            "targetAddress",
                            "metadataValue"
                        ],
                        "managerGetter": "namespace/metadatas",
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "metadataEntriesError"
                    }
                ]
            }
        },
        {
            "path": "/transaction/:transactionHash",
            "name": "transaction-detail",
            "meta": {
                "group": "detail-page",
                "keepAliveGoTo": []
            },
            "props": {
                "schema": [
                    {
                        "type": "Card",
                        "title": "transactionInfoTitle",
                        "fullWidth": true,
                        "body": "TableInfoView",
                        "fields": [
                            "blockHeight",
                            "transactionHash",
                            "transactionId",
                            "date",
                            "deadline",
                            "effectiveFee",
                            "signature",
                            "signer",
                            "status",
                            "confirm"
                        ],
                        "managerGetter": "transaction/info",
                        "pagination": false,
                        "errorMessage": "transactionInfoError"
                    },
                    {
                        "type": "Card",
                        "title": "transactionDetailTitle",
                        "fullWidth": false,
                        "body": "TableInfoView",
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/transactionDetail",
                        "pagination": false,
                        "errorMessage": "transactionDetailError"
                    },
                    {
                        "type": "Card",
                        "title": "mosaicsTitle",
                        "fullWidth": false,
                        "body": "TableListView",
                        "fields": [
                            "mosaicId",
                            "amount",
                            "mosaicAliasName"
                        ],
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/transferMosaics",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "transactionMosaicsError"
                    },
                    {
                        "type": "Card",
                        "title": "aggregateInnerTransactionsTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "transactionId",
                            "transactionDescriptor",
                            "signer",
                            "transactionBody"
                        ],
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/aggregateInnerTransactions",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "aggregateInnerTransactionsError"
                    },
                    {
                        "type": "Card",
                        "title": "aggregateCosignaturesTitle",
                        "fullWidth": true,
                        "body": "TableListView",
                        "fields": [
                            "signature",
                            "signer"
                        ],
                        "managerGetter": "transaction/info",
                        "dataGetter": "transaction/aggregateCosignatures",
                        "hideEmptyData": true,
                        "pagination": true,
                        "pageSize": 5,
                        "errorMessage": "aggregateCosignaturesError"
                    }
                ]
            }
        }
    ]
}